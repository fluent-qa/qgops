{
  "actions": [
    {
      "name": "java",
      "desc": "Create Java Template Project",
      "commands": [
        "git clone https://github.com/fluent-qa/fluent-java-tpl",
        "cd fluent-java-tpl && mv .git git_bnk"
      ]
    },
    {
      "name": "go",
      "desc": "Create Go Template Project",
      "commands": [
        "git clone https://github.com/fluent-qa/fluentqa-gotpl.git --depth=1",
        "cd fluentqa-gotpl && mv .git git_bnk "
      ]
    },
    {
      "name": "ts-lib",
      "desc": "Create typescript lib Template Project",
      "commands": [
        "git clone https://github.com/fluent-qa/ts-lib-starter",
        "cd ts-lib-starter && mv.git git_bnk"
      ]
    },
    {
      "name": "fe-next",
      "desc": "Create next shacdn-ui project",
      "commands": [
        "git clone https://github.com/fluent-qa/fe-next-starter",
        "cd fe-next-starter && mv.git git_bnk"
      ]
    },
    {
      "name": "mono",
      "desc": "Create typescript mono Template Project",
      "commands": [
        "git clone https://github.com/qdriven/mono-ts-starter.git",
        "cd mono-ts-starter && mv.git git_bnk"
      ]
    },
    {
      "name": "python",
      "desc": "Create Python Template Project",
      "commands": [
        "pdm init -n https://github.com/fluent-qa/fluentqa-pytpl.git",
        "rm -rf templates/"

      ]
    }
  ]
}